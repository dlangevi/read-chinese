{
  "name": "read-chinese",
  "version": "0.1.0",
  "private": true,
  "description": "Tool for creating and managing flashcards to learn chinese",
  "author": "dlangevi",
  "scripts": {
    "dev": "vite",
    "lint": "eslint src",
    "typecheck": "vue-tsc --noEmit",
    "build:vite": "vue-tsc --noEmit && vite build",
    "build": "yarn package && yarn build:vite",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:node": "tsc -p src-node/tsconfig.json --outDir src-node/build",
    "package": "yarn build:node && pkg src-node/package.json -o src-tauri/binaries/app && node scripts/move-binary.js",
    "package:debug": "yarn build:node && pkg src-node/package.json --debug -o src-tauri/binaries/app && node scripts/move-binary.js",
    "migrate": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile knexfile.ts"
  },
  "dependencies": {
    "@azure/cognitiveservices-imagesearch": "^3.0.1",
    "@azure/ms-rest-azure-js": "^2.1.0",
    "@commonify/lowdb": "^3.0.0",
    "@tauri-apps/api": "^1.1.0",
    "@vicons/ionicons5": "^0.12.0",
    "ag-grid-community": "^28.1.1",
    "ag-grid-vue3": "^28.1.1",
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "knex": "^2.3.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.24.0",
    "naive-ui": "^2.33.5",
    "node-calibre": "^2.1.1",
    "node-fetch-commonjs": "^3.0.0",
    "nodejieba": "^2.6.0",
    "pinia": "^2.0.23",
    "sqlite3": "^5.1.2",
    "tmp": "^0.2.1",
    "vite-plugin-windicss": "^1.8.8",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5",
    "windicss": "^3.5.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.1.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.14",
    "@types/node": "18.0.0",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.6.0",
    "execa": "5.1.1",
    "lint-staged": "^13.0.3",
    "pkg": "5.2.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.2",
    "vue-tsc": "^0.40.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "license": "MIT"
}
